{
	"name": "plop-generator",
	"displayName": "Plop Generator",
	"description": "Generate files using plop templates",
	"version": "0.0.1",
	"publisher": "jpmartinez",
	"engines": {
		"vscode": "^1.34.0"
	},
	"categories": [
		"Other",
		"Snippets"
	],
	"keywords": [
		"Plop",
		"Scaffold"
	],
	"author": {
		"name": "jpmartinez"
	},
	"activationEvents": [
		"onCommand:plopgenerator.generate"
	],
	"icon": "./images/plop.png",
	"main": "./src/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "plopgenerator.generate",
				"title": "Generate here with Plop"
			}
		],
		"menus": {
			"explorer/context": [
				{
					"command": "plopgenerator.generate",
					"group": "navigation@500"
				}
			]
		}
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/jpmartinez/plop-generator.git"
	},
	"scripts": {
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"@types/mocha": "^2.2.42",
		"@types/node": "^10.12.21",
		"eslint": "^5.13.0",
		"typescript": "^3.3.1",
		"vscode": "^1.1.28",
		"prettier": "^1.17.1"
	},
	"dependencies": {
		"node-plop": "^0.18.0"
	},
	"prettier": {
		"useTabs": true
	}
}
